    "#!/usr/bin/python3\n",
    "#to draw a circle of 8 cm \n",
    "import turtle\n",
    "t = turtle.Turtle()\n",
    "pixels = ( 96 * 1)/2.54\n",
    "r = 4*pixels\n",
    "r = r /1.1\n",
    "r=r*1.02\n",
    "t.clear()\n",
    "t.circle(r)"
   ]
